<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="appenderConsole" target="SYSTEM_OUT">
			​
			<PatternLayout
				pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
			​
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="tp2" level="error" additivity="false">
			​
			<AppenderRef ref="appenderConsole" />
						<AppenderRef ref="appenderConsole" />
			​
		</Logger>
		​
		<Logger name="tp" level="error" additivity="false">
			​
			<AppenderRef ref="appenderConsole" />
			​
		</Logger>
		​
		<Root level="debug">
			​
			<AppenderRef ref="appenderConsole" />
			​
		</Root>
	</Loggers>
	​
</Configuration>